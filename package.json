{
  "name": "timetracking",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "npm": "please-use-yarn",
    "node": ">= 20.11.0",
    "yarn": ">= 4.1.0"
  },
  "volta": {
    "node": "20.11.0",
    "yarn": "4.1.0"
  },
  "packageManager": "yarn@4.1.0",
  "scripts": {
    "prepare": "yarn generate:api-client",
    "dev": "yarn generate:api-client && vite",
    "build": "yarn generate:api-client && vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "format": "prettier --write . --ignore-path .gitignore",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "generate:api-client": "openapi --input ./api.yaml --output ./src/apiClient --useOptions"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.22",
    "@tailwindcss/container-queries": "^0.1.1",
    "@vueuse/components": "^10.10.0",
    "@vueuse/core": "^10.10.0",
    "clsx": "^2.1.1",
    "core-js": "^3.37.1",
    "happy-dom": "^14.11.1",
    "inert-polyfill": "^0.2.5",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "sass": "^1.77.2",
    "vue": "^3.4.27",
    "vue-query": "1.26.0",
    "vue-router": "^4.3.2",
    "vue-toastification": "^2.0.0-rc.5",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.6",
    "@eslint/js": "^9.3.0",
    "@rushstack/eslint-patch": "^1.10.3",
    "@types/connect": "^3.4.38",
    "@types/jsdom": "^21.1.6",
    "@types/lodash": "^4.17.4",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.12.12",
    "@types/sortablejs": "^1.15.8",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/parser": "^7.10.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/cli-plugin-babel": "~5.0.8",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.19",
    "babel-loader": "^9.1.3",
    "cross-env": "^7.0.3",
    "eslint": "^9.3.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^3.2.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-vue": "^9.26.0",
    "jsdom": "^24.1.0",
    "lint-staged": "^15.2.5",
    "msw": "^2.3.0",
    "openapi-typescript-codegen": "^0.29.0",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.10.0",
    "vite": "^5.2.11",
    "vitest": "^1.6.0",
    "vue-loader": "^17.4.2",
    "vue-tsc": "^2.0.19"
  },
  "resolutions": {
    "jackspeak": "2.1.1"
  },
  "msw": {
    "workerDirectory": "./public"
  }
}
